    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'sandbox_rust_torch'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=sandbox_rust_torch",
                    "--package=sandbox_rust_torch"
                ],
                "filter": {
                    "name": "sandbox_rust_torch",
                    "kind": "bin"
                }
            },
            "args": ["resnet34.ot","mig.jpg"],
            "cwd": "${workspaceFolder}"
        },
       
        {
            "type": "lldb",
            "name": "Debug unit tests in executable 'sandbox_rust_torch'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=sandbox_rust_torch",
                    "--package=sandbox_rust_torch"
                ],
                "filter": {
                    "name": "sandbox_rust_torch",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}